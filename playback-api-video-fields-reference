<section class="bcls-section">
<h2 id="link_Object">link Object</h2>

<table class="bcls-table">
	<caption>Table 4: link Object Fields</caption>
	<thead class="bcls-table__head">
		<tr>
			<th>Field</th>
			<th>Type</th>
			<th>Default</th>
			<th>Description</th>
		</tr>
	</thead>
	<tbody class="bcls-table__body">
		<tr>
			<td><code>text</code></td>
			<td>String</td>
			<td>null</td>
			<td>Display text for the link</td>
		</tr>
		<tr>
			<td><code>url</code></td>
			<td>String</td>
			<td>null</td>
			<td>URL for the link</td>
		</tr>
